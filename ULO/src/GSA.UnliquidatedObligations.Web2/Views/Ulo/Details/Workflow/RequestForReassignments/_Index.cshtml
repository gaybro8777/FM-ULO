@using GSA.UnliquidatedObligations.Web.Controllers
@model GSA.UnliquidatedObligations.Web.Models.WorkflowViewModel
@{
    var rfr = Model.RequestForReassignment;
    if (rfr==null)
    {
        Html.RenderAction(RequestForReassignmentsController.ActionNames.Details, RequestForReassignmentsController.Name, new {workflowId = Model.Workflow.WorkflowId, uloRegionId = Model.Workflow.UnliquidatedObligation.RegionId, wfDefintionOwnerName = Model.WorkflowDescriptionViewModel.CurrentActivity.OwnerUserName });
    }
    else
    {
        Html.RenderAction(RequestForReassignmentsController.ActionNames.Details, RequestForReassignmentsController.Name, new { id = rfr.RequestForReassignmentID, workflowId = Model.Workflow.WorkflowId, uloRegionId = Model.Workflow.UnliquidatedObligation.RegionId });
    }
}
